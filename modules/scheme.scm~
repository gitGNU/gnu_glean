(define-module (modules scheme)
  )

(define (predicate-q question) 
  (string-append "What is the predicate for " question))

(define problem1 
  (problem "What is the predicate for the empty list?" "null?"))
(define problem2
  (problem "What is the predicate for numbers?" "number?"))
(define problem3
  (problem (predicate-q "strings?") "string?"))
(define problem4
  (problem (predicate-q "boolean?") "string?"))

